{
  "name": "plooral-test",
  "version": "1.0.0",
  "author": "Rafael Fernandes Marques",
  "repository": {
    "url": "https://github.com/rafaelfmarques/backend-developer-test/tree/rafael-backend-developer-test"
  },
  "scripts": {
    "setup": "npm i && docker-compose up --build --detach && npx prisma generate && npx prisma migrate dev && npx prisma db seed",
    "build": "tsc && tsc-alias",
    "build-railway": "npx prisma migrate dev && npx prisma db seed && ts-node swagger-config.ts && nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register src/main.ts' -e ts",
    "test:staged": "jest __tests__/@e2e --passWithNoTests --detectOpenHandles  --no-cache",
    "test": "jest",
    "start": "ts-node swagger-config.ts && nodemon",
    "dev": " ts-node swagger-config.ts && nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register src/main.ts' -e ts",
    "prepare": "husky",
    "eslint:fix": "npx eslint 'src/**' --fix",
    "prettier:check": "npx prettier --check 'src/**'",
    "swagger-autogen": "ts-node swagger-config.ts",
    "serverless:publish": "serverless invoke local --function publishJobs",
    "serverless:unpublish": "serverless invoke local --function unpublishJobs"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.511.0",
    "@aws-sdk/client-sqs": "^3.511.0",
    "@aws-sdk/credential-providers": "^3.511.0",
    "@prisma/client": "^5.9.1",
    "@swc/core": "^1.4.1",
    "@swc/jest": "^0.2.36",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "aws-lambda": "^1.0.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.2",
    "express": "^4.18.2",
    "node-cache": "^5.1.2",
    "openai": "^4.28.0",
    "redis": "^4.6.13",
    "swagger-ui-express": "^5.0.0",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.133",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "prisma": "^5.9.1",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.51.0",
    "serverless-offline": "^13.3.3",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^2.1.5",
    "supertest": "^6.3.4",
    "swagger-autogen": "^2.23.7",
    "swagger-jsdoc": "^6.2.8",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0"
  }
}
